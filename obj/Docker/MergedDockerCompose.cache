name: dockercompose8905556348347886270
services:
  shopping.api:
    build:
      context: D:\AKS learn\.Net Microservice\Shopping.Docker
      dockerfile: Shopping.API/Dockerfile
    container_name: shoppingapi
    depends_on:
      shopping.db:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      DatabaseSettings:CollectionName: products
      DatabaseSettings:ConnectionString: mongodb://shoppingdb:27017
      DatabaseSettings:DatabaseName: CatalogDb
    image: shoppingapi
    networks:
      default: null
    ports:
      - mode: ingress
        target: 80
        published: "4000"
        protocol: tcp
  shopping.client:
    build:
      context: D:\AKS learn\.Net Microservice\Shopping.Docker
      dockerfile: Shopping.Client/Dockerfile
    container_name: shoppingclient
    depends_on:
      shopping.api:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ShoppingUrl: http://shoppingapi
    image: shoppingclient
    networks:
      default: null
    ports:
      - mode: ingress
        target: 80
        published: "5000"
        protocol: tcp
  shopping.db:
    container_name: shoppingdb
    image: mongo:5.0.23
    networks:
      default: null
    ports:
      - mode: ingress
        target: 27017
        published: "27017"
        protocol: tcp
    restart: always
networks:
  default:
    name: dockercompose8905556348347886270_default